<Window x:Class="HashtableApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashtableApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Add_Button" Content="Add" HorizontalAlignment="Left" Height="35" Margin="228,260,0,0" VerticalAlignment="Top" Width="79" Click="Add_Button_Click"/>
        <Button x:Name="Remove_Button" Content="Remove" HorizontalAlignment="Center" Height="35" Margin="0,260,0,0" VerticalAlignment="Top" Width="80" Click="Remove_Button_Click"/>
        <Button x:Name="ShowAll_Button" Content="Show All" HorizontalAlignment="Left" Height="35" Margin="495,260,0,0" VerticalAlignment="Top" Width="80" Click="ShowAll_Button_Click"/>
        <TextBox x:Name="ValueBox" HorizontalAlignment="Left" Height="19" Margin="228,217,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="347"/>
        <TextBox x:Name="KeyBox" HorizontalAlignment="Left" Height="19" Margin="228,176,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="347"/>

    </Grid>
</Window>
